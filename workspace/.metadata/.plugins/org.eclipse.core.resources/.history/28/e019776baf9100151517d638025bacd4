package Tasks;

public class Task05 {

	double getAverage(int[] array){
		int i=0;
		double sum=0;
		for(i=0;i<array.length;i++){
			sum=sum+array.length;
		}
		
		return sum/array.length;
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Task05 test = new Task05();

	}

}
